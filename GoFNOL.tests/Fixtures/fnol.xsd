						<xs:documentation>Unique transaction id sent by the Sender</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ADP_TRANSACTION_ID" type="IntegerOrBlankType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ADP Transaction ID is the key returned with a successful ADD assignment request. For Message types-- Change (CHG), CAN (Cancel) and SUP (Supplement) this key is required. For internal reference ADP Transaction ID is the Work Assignment PK.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CLIENT_PASSTHRU_DATA" type="xs:anyType" minOccurs="0"/>
				<xs:element name="ASSIGNED_TO" type="AssignedToType"/>
