				<xs:element name="MESSAGE_TYPE" type="MessageTypeType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Type of Message with codes as indicated ADD: Add Request
CHG: Change Request CAN: Cancel Request SUP: Supplement Request</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SENDER_TRANSACTION_ID" minOccurs="0">
